<h1>New Wiki</h1>

<div class="col-md-8">
  <%= form_for :wiki do |f| %>
    <%= f.text_field :title, class: 'form-control', placeholder: "Enter wiki title"%><br />
    <%= f.text_area :body, rows: 8, class: 'form-control', placeholder: "Enter wiki body"%><br />
    <div class="form-group">
      <%= f.submit "Save", class: 'btn btn-success' %>
    </div>
  <% end %>
</div>
